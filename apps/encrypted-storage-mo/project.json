{
  "name": "encrypted-storage-mo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/encrypted-storage-mo",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/encrypted-storage-mo",
        "command": "dfx build example",
        "parallel": false
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/encrypted-storage-mo",
        "command": "vitest",
        "parallel": false
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/encrypted-storage-mo",
        "command": "lintoko",
        "parallel": false
      }
    },
    "doc": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/encrypted-storage-mo",
        "commands": ["$(dfx cache show)/mo-doc --source src --format plain"],
        "parallel": false
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/encrypted-storage-mo",
        "command": "dfx deploy encrypted-storage",
        "parallel": false
      }
    }
  }
}
