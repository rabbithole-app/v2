{
  "name": "icpay-webhooks-mo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/icpay-webhooks-mo",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/icpay-webhooks-mo",
        "command": "docker compose exec -T replica bash -lc 'dfx build example'",
        "parallel": false
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/icpay-webhooks-mo",
        "command": "docker compose exec -T replica bash -lc 'cd /app && npx vitest'",
        "parallel": false
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/icpay-webhooks-mo",
        "command": "docker compose exec -T replica bash -lc 'lintoko'",
        "parallel": false
      }
    }
  }
}
