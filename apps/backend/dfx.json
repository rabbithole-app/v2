{
  "canisters": {
    "rabbithole-backend": {
      "declarations": {
        "bindings": ["js", "ts"],
        "node_compatibility": true
      },
      "main": "src/main.mo",
      "type": "motoko"
    },
    "rabbithole-frontend": {
      "dependencies": [],
      "source": ["dist"],
      "type": "assets",
      "frontend": {
        "entrypoint": "index.html"
      },
      "build": ["bash scripts/build-frontend.sh"]
    },
    "encrypted-storage": {
      "declarations": {
        "bindings": ["js", "ts"],
        "node_compatibility": true
      },
      "main": "src/EncryptedStorageCanister.mo",
      "type": "motoko",
      "args": "--max-stable-pages 3276800"
    },
    "internet-identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2025-11-21/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/download/release-2025-11-21/internet_identity_dev.wasm.gz",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    },
    "icp-ledger": {
      "type": "custom",
      "candid": "https://github.com/dfinity/ic/releases/download/ledger-suite-icp-2025-08-29/ledger.did",
      "wasm": "https://github.com/dfinity/ic/releases/download/ledger-suite-icp-2025-08-29/ledger-canister_notify-method.wasm.gz",
      "remote": {
        "id": {
          "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
        }
      }
    },
    "cmc": {
      "type": "custom",
      "candid": "canisters/cmc/cycles-minting-canister.did",
      "wasm": "canisters/cmc/cycles-minting-canister-custom.wasm",
      "remote": {
        "id": {
          "ic": "rkp4c-7iaaa-aaaaa-aaaca-cai"
        }
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
