{
  "canisters": {
    "rabbithole-backend": {
      "declarations": {
        "bindings": ["js", "ts"],
        "node_compatibility": true,
        "output": "declarations/rabbithole-backend"
      },
      "main": "src/main.mo",
      "type": "motoko",
      "gzip": true,
      "init_arg" : "(record { github = opt record { apiUrl = \"http://mock-server:8080\"; owner = \"user\"; repo = \"repo\"; token = null : opt text } })"
    },
    "rabbithole-frontend": {
      "dependencies": [],
      "source": ["dist"],
      "type": "assets",
      "frontend": {
        "entrypoint": "index.html"
      },
      "build": ["bash scripts/build-frontend.sh"]
    },
    "encrypted-storage": {
      "declarations": {
        "bindings": ["js", "ts"],
        "node_compatibility": true,
        "output": "declarations/encrypted-storage"
      },
      "main": "src/EncryptedStorageCanister.mo",
      "type": "motoko",
      "args": "--max-stable-pages 3276800",
      "gzip": true
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
