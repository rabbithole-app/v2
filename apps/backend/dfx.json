{
  "canisters": {
    "rabbithole-backend": {
      "declarations": {
        "bindings": ["js", "ts"],
        "node_compatibility": true
      },
      "main": "src/main.mo",
      "type": "motoko"
    },
    "rabbithole-frontend": {
      "dependencies": ["rabbithole-backend"],
      "source": ["./dist"],
      "type": "assets",
      "build": ""
    },
    "encrypted-storage": {
      "declarations": {
        "bindings": ["js", "ts"],
        "node_compatibility": true
      },
      "main": "src/EncryptedStorageCanister.mo",
      "type": "motoko",
      "args": "--max-stable-pages 3276800"
    },
    "internet-identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2025-08-15/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/download/release-2025-08-15/internet_identity_dev.wasm.gz",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
