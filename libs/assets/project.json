{
  "name": "assets",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/assets/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "swc libs/assets/src -d dist/libs/assets/cjs --config-file libs/assets/.swcrc.cjs.json",
          "swc libs/assets/src -d dist/libs/assets/esm --config-file libs/assets/.swcrc.esm.json"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}
